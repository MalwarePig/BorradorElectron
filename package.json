{
  "name": "nobl-E Soft",
  "productName": "Promesa",
  "version": "1.0.0",
  "description": "Creador de Cotizaciones",
  "main": "src/index.js",
  "scripts": {
    "start": "SET NODE_ENV=development & electron src/index.js",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "SET NODE_ENV=production & electron-packager . Nobl-E Soft --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out= Promesa App --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"PROMESA Bin\"",
    "package-linux": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png --prune=true --out=release-builds"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "adm-zip": "^0.5.9",
    "electron": "^15.3.0",
    "electron-packager": "^15.4.0",
    "electron-reload": "^2.0.0-alpha.1",
    "jquery": "^3.6.0",
    "jspdf": "^2.4.0",
    "jspdf-autotable": "^3.5.23",
    "moment": "^2.29.1",
    "read-excel-file": "^5.2.20",
    "sweetalert2": "^11.1.10",
    "zip-a-folder": "^1.1.0"
  },
  "devDependencies": {
    "electron-rebuild": "^3.2.3"
  }
